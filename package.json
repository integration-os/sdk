{
  "name": "@integrationos/poc-sdk",
  "version": "0.3.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "poc-sdk",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.15.2",
    "ava": "^5.1.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build-all": "napi build --platform --release && napi build --target aarch64-apple-darwin --platform --release npm/darwin-arm64 && napi build --target x86_64-apple-darwin --platform --release npm/darwin-x64 && napi build --target x86_64-unknown-linux-gnu --platform --release npm/linux-x64-gnu",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "test": "ava",
    "version": "napi version"
  },
  "packageManager": "yarn@3.5.1",
  "optionalDependencies": {
    "@integrationos/poc-sdk-darwin-arm64": "0.3.0",
    "@integrationos/poc-sdk-darwin-x64": "0.3.0",
    "@integrationos/poc-sdk-linux-x64-gnu": "0.3.0"
  }
}
